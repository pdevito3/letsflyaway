{
  "environments": [
    {
      "id": "fcGeneral",
      "name": "FC General",
      "region": "us-east-1",
      "source": {
        "branch": "master"
      },
      "services": [
        {
          "id": "letsFlyAwayApi",
          "name": "letsFlyAwayApi",
          "type": "fargate",
          "cpu": 0.25,
          "memory": 512,
          "dockerfilePath": "./RecipeManagement/src/RecipeManagement/Dockerfile",
          "dockerContext": "./RecipeManagement/src/RecipeManagement",
          "port": 8080,
          "minInstances": 1,
          "maxInstances": 1,
          "envVariables": {
            "PORT": "8080",
            "ASPNETCORE_ENVIRONMENT": "Development",
            "DB_CONNECTION_STRING": {
              "fromService": {
                "id": "letsFlyAwayDb",
                "value": "dbConnectionString"
              }
            }
          }
        },
        {
          "id": "letsFlyAwayDb",
          "name": "letsFlyAwayDb",
          "type": "rds",
          "engine": "postgres",
          "engineVersion": "12",
          "instanceSize": "db.t2.micro",
          "storage": 20,
          "private": false
        }
      ]
    }
  ]
}